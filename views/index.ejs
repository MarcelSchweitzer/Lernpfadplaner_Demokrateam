<div class="modal fade" id="CookieBanner" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header"> 
            </div>
            <div class="modal-body">
                <p  style="font-size: 20px; font-weight: 500;">Wir verwenden Cookies </p>
                <p  style="font-size: 10px;">
                    <%= data.cookieText %>
                </p>
            </div>
            <div class="modal-footer">
                <button type="button" style="margin: 0 auto;" class="btn btn-light allowCookie" id="allowCookie" data-dismiss="modal">Erlauben</button>
                <button type="button" class="btn btn-light denyCookie" id="denyCookie" data-dismiss="modal">Abbrechen</button>
            </div>
        </div>
    </div>
</div>
