/>
<header>
    <div class="container-fluid">
        <div class="row">
            <div class="col-3">
                <div class="setting">
                    <a href="#"> <img type="image" class="button setting-icon" id="settingsBtn" src="img/settings.svg" alt="settings.svg"> </a>
                </div>
            </div>
            <div class="col-9">
                <div class="colz-icon">
                    <a href="#" id="usernameText">
                        <%= data.nickname %>
                    </a>
                    <a href="#"  data-toggle="modal" data-target="#exampleModal">
                        <img class="button" src="img/download.svg">
                    </a>
                    <div class="modal fade" id="exampleModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
                        <div class="modal-dialog" role="document">
                          <div class="modal-content">
                            <div class="modal-header">
                              <h5 class="modal-title" id="exampleModalLabel">Speichern</h5>
                              <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                              </button>
                            </div>
                            <div class="modal-body">
                                <button type="button" id="exportButton" class="btn btn-secondary" data-dismiss="modal">Pdf</button>
                                <button type="button" id="downloadButton" class="btn btn-secondary">Json</button>
                            </div>
                          
                          </div>
                        </div>
                      </div>
                    <a href="#">
                        <img type="image" class="button save-icon" id="homeBtn" src="img/home.svg" alt="home.svg">
                    </a>
                </div>
            </div>
        </div>
    </div>
 
</header>
