<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap/4.6.1/css/bootstrap.min.css" integrity="sha512-T584yQ/tdRR5QwOpfvDfVQUidzfgc2339Lc8uBDtcp/wYu80d7jwBgAxbyMh0a9YM9F8N3tdErpFI8iaGx6x5g==" crossorigin="anonymous" referrerpolicy="no-referrer"
/>
<header>
        <div class="row head">
            <div class="col-8" style="padding:0px;">
                <div class="home">
                    <a href="#">
                        <img type="image" class="button save-icon" id="homeBtn" src="img/home.svg" alt="home.svg">
                    </a>
                    <img id="userIcon" src="img/user.svg">
                   <a href="#" id="usernameText">
                        <%= data.nickname %>
                    </a>   
                </div>
            </div>
            <div class="col-4" style="padding:0px;">
                <div class="colz-icon">
                    <a href="#"  data-toggle="modal" data-target="#downloadPop">
                        <img class="button" src="img/download.svg">
                    </a>
                    
                    <a href="#"> <img type="image" class="button setting-icon" id="settingsBtn" src="img/settings.svg" alt="settings.svg"> </a>
                </div>
            </div>
        </div>
</header>
<div class="modal fade" id="downloadPop" tabindex="-1" role="dialog" aria-labelledby="downloadPopLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="downloadPopLabel">Speichern</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body" id="downloadPop">
          <button type="button" id="exportButton" class="btn btn-secondary" data-dismiss="modal">Pdf</button>
          <button type="button" id="downloadButton" class="btn btn-secondary">Json</button>
      </div>
    </div>
  </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
